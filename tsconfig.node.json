{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "types": ["node"],
    "noEmit": true,
    "ignoreDeprecations": "6.0"
  },
  "include": ["vite.config.js", "vite.config.ts"]
}
